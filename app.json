{
  "expo": {
    "name": "Narzędziownia",
    "slug": "mobilna",
    "scheme": "narzedziownia",
    "version": "1.6.1",
    "extra": {
      "apiBaseUrl": "http://192.168.10.97:3000",
      "EXPO_PUBLIC_DISABLE_HISTORY_FETCH": "false",
      "eas": {
        "projectId": "2b0d8aaa-dada-4b64-a737-03f7c55b9012"
      }
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#363636"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Aplikacja potrzebuje dostępu do kamery, aby skanować kody QR i kreskowe."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#363636"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.dbrzezinsky.narzedziownia",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-system-ui",
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Aplikacja potrzebuje dostępu do kamery, aby skanować kody QR i kreskowe."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "owner": "dbrzezinsky",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2b0d8aaa-dada-4b64-a737-03f7c55b9012"
    }
  }
}
