{
  "expo": {
    "name": "Narzędziownia",
    "slug": "narzedziownia",
    "version": "1.3.0",
    "extra": {
      "apiBaseUrl": "http://192.168.10.99:3000",
      "EXPO_PUBLIC_DISABLE_HISTORY_FETCH": "false",
      "eas": {
        "projectId": "2b0d8aaa-dada-4b64-a737-03f7c55b9012"
      }
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Aplikacja potrzebuje dostępu do kamery, aby skanować kody QR i kreskowe."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.dbrzezinsky.narzedziownia"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Aplikacja potrzebuje dostępu do kamery, aby skanować kody QR i kreskowe."
        }
      ]
    ],
    "owner": "dbrzezinsky"
  }
}
